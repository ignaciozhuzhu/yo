/*  定义  */
html,body{height:100%;overflow:auto;}
div,form,p,ul,li,ol,dl,dt,dd,h1,h2,h3,h4,h5,h6{ margin:0px;padding:0px;text-align:left;}
ul,ol,li,dl,dt,dd{list-style:none;}
body {
	width:100%;
	height:100%;
	color: #000;
	font: normal 14px "Microsoft Yahei", Arial, Helvetica, sans-serif;
	line-height: 1.7em;
	background: #f6f6f6;
	padding:0;
	margin:0;
}
a:link{text-decoration:none;}
a:visited{text-decoration:none;}
a:hover{text-decoration:none;}
a:active{text-decoration:none;}
img{vertical-align:middle;}
.weui_flex{
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
}
.weui_flex_item{
	-webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
}
.center-block{
	display: block;
	margin-left:auto;
	margin-right:auto;	
}
.text-align{text-align:center;}
.img-circle {
  border-radius:50%;
  display: inline-block;
  max-width: 100%;
  padding: 0px;
  line-height: 1.42857143;
  background-color: #fff;
  border: 1px solid #ddd;
  -webkit-transition: all .2s ease-in-out;
       -o-transition: all .2s ease-in-out;
          transition: all .2s ease-in-out;
}
.img-thumbnail {
  display: inline-block;
  max-width: 100%;
  height: auto;
  padding: 4px;
  line-height: 1.42857143;
  background-color: #fff;
  border: 1px solid #ddd;
  border-radius: 4px;
  -webkit-transition: all .2s ease-in-out;
       -o-transition: all .2s ease-in-out;
          transition: all .2s ease-in-out;
}
.img-rounded {
  border-radius: 50%;
}
.img-responsive,
.thumbnail > img,
.thumbnail a > img,
.carousel-inner > .item > img,
.carousel-inner > .item > a > img {
  display: block;
  max-width: 100%;
  height: auto;
}
.container-fluid {
  padding-right: 15px;
  padding-left: 15px;
  margin-right: auto;
  margin-left: auto;
}

.row {
  margin-right: -15px;
  margin-left: -15px;
}

/*不换行超出变...*/
.wrap-class{
	white-space:nowrap;
	overflow:hidden;
	text-overflow:ellipsis;
}
.wrap-class-2{
	overflow:hidden; 
	text-overflow:ellipsis;
	display:-webkit-box; 
	-webkit-box-orient:vertical;
	-webkit-line-clamp:2; 
}

